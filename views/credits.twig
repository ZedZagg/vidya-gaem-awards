{% extends 'master.twig' %}

{% block title %}Credits{% endblock %}

{% block content %}
<style type="text/css">
  @font-face {
    font-family: 'modern typewriter';
    src: url('/fonts/modern typewriter.eot');
    src: local('☺'), url('/fonts/modern typewriter.woff') format('woff'), url('/fonts/modern typewriter.ttf') format('truetype'), url('/fonts/modern typewriter.svg') format('svg');
    font-weight: normal;
    font-style: normal;
  }
  @font-face {
    font-family: 'yoster';
    src: url('/fonts/yoster.eot');
    src: local('☺'), url('/fonts/yoster.woff') format('woff'), url('/fonts/yoster.ttf') format('truetype'), url('/fonts/yoster.svg') format('svg');
    font-weight: normal;
    font-style: normal;
  }

  html {
    background: black url("/2015voting/bg.gif");
  }

  body {
    background: none;
  }

  h1, h2 {
    margin:0;
    padding:0;
    color: #f85898;
    font-weight: normal;
  }

  h1 {
    font-family: "modern typewriter", Tahoma, sans-serif;
    text-transform: uppercase;
    font-size: 3em;
    line-height: 100%;
  }
  h2 {
    font-family: "yoster", Calibri, sans-serif;
    font-size: 1.3em;
    line-height: 100%;
    color: #bfbfbf;
    -webkit-font-smoothing: none;
  }

  p {
    font-family: "modern typewriter", Tahoma, sans-serif;
    font-size: 20px;
    line-height: 1.3em;
    color: #f85898;
    margin-bottom: 10px;
  }

  .category ul {
    list-style-type: none;
    margin: 0;
    font-size: 24px;
  }

  .category ul li {
    line-height: 1.5em;
    font-family: "modern typewriter", Calibri, sans-serif;
    color: #00ebdb;
  }

  .category ul li:first-child {
    font-weight: bold;
    font-size: 36px;
    font-family: "modern typewriter", Tahoma, sans-serif;
    color: #f7d059;
  }

  .category ul li:last-child {
    color: #f85898;
    font-size: 18px;
    font-family: "modern typewriter", Tahoma, sans-serif;
  }

  .zig {
    clear: both;
    height: 20px;
    margin: 30px 0;
    background-image: url("/2015voting/zig_horizontal.gif");
  }

  .awardHeaderContainer:first-child {
    margin-top: 0;
  }

  .awardHeaderContainer {
    margin-top: 45px;
    padding-top: 24px;
    height: 90px;
    background: url(/2015voting/title_bg.gif) top center no-repeat;
    background-size: 100%;
    text-align: center;
  }
  .awardName {
    font-family: "modern typewriter", Tahoma, sans-serif;
    text-transform: uppercase;
    font-size: 2em;
    line-height: 35px;
    color: #00ebdb;
  }

  a, a:visited {
    color: #00ebdb;
  }

  a:hover, a:active {
    color: #f85898;
  }

  .awardSubtitle {
    color: #00ebdb;
  }

  .navbar, .navbar .navbar-inner {
    -moz-box-shadow:    none;
    -webkit-box-shadow: none;
    box-shadow:         none;
    border:             none;
    background: black;
  }

  .navbar a {
    font-family: "modern typewriter", Tahoma, sans-serif !important;
    color: white !important;
    text-shadow: none !important;
    background: none !important;
  }

  .navbar a:hover {
    color: #f85898 !important;
  }

  .navbar .nav>.active>a {
    box-shadow: inset 0 3px 15px rgba(255, 255, 255, 0.125);
    -webkit-box-shadow: inset 0 3px 15px rgba(255, 255, 255, 0.125);
    -moz-box-shadow: inset 0 3px 15px rgba(255, 255, 255, 0.125);
  }

  .navbar-fixed-top {
    border-bottom: 1px solid #f7d059;
  }

  .navbar-fixed-bottom {
    border-top: 1px solid #f7d059;
  }

  .navbar-brand {
    font-weight: bold;
  }

  .credits {
    text-align: center;
    font-size: 20px;
    line-height: 30px;
  }

  .credits strong {
    color: #f7d059;
    font-size: 22px;
  }

  .FLAOT {
    display: inline-block;
    margin: 0 30px;
  }

  .specialThanks {
    background-size: auto;
    height: 100px;
    padding-top: 38px;
    font-size: x-large;
    margin-bottom: 20px;
  }
</style>

<div class="zig"></div>

<div class="row">
  <div class="col-md-5" style="text-align: center;">
    <img src="/2015voting/logo.gif" alt="Logo of the 2015 Vidya Gaem Awards">
  </div>
  <div class="col-md-7" style="text-align: center; padding-top: 30px;">
