{
    "name": "vidya-gaem-awards/website",
    "description": "Website for the Vidya Gaem Awards",
    "license": "MIT",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "php": "^8.1",
        "beberlei/doctrineextensions": "^1.1",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^2",
        "ehesp/steam-login": "^1.0.2",
        "fightbulc/moment": "^1.17.1",
        "knojector/steam-authentication-bundle": "^1.3.2",
        "koraktor/steam-condenser": "dev-master",
        "league/csv": "^9.6",
        "paragonie/random-lib": "^2.0.0",
        "sensio/framework-extra-bundle": "^6",
        "sensiolabs/ansi-to-html": "^1.1.3",
        "symfony/asset": "6.1.*",
        "symfony/console": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/dom-crawler": "6.1.*",
        "symfony/flex": "^1.1",
        "symfony/form": "6.1.*",
        "symfony/framework-bundle": "6.1.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/runtime": "6.1.*",
        "symfony/security-bundle": "6.1.*",
        "symfony/serializer": "6.1.*",
        "symfony/twig-bundle": "6.1.*",
        "symfony/validator": "6.1.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "6.1.*",
        "tiben/crontab-manager": "~1.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpunit/phpunit": "^9.4",
        "symfony/debug-bundle": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "6.1.*",
        "symfony/stopwatch": "6.1.*",
        "symfony/web-profiler-bundle": "6.1.*",
        "vimeo/psalm": "^4.30"
    },
    "replace": {
        "paragonie/random_compat": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "6.1.*"
        }
    }
}
